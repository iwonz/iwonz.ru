{
	"name": "iwonz.ru",
	"exportDir": "./dist",
	"minify": true,
	"compile": true,
	"sourceMap": false,
	"exclusions": [
		{
			"path": "node_modules",
			"action": "exclude",
			"type": "dir"
		},
		{
			"path": "template",
			"action": "exclude",
			"type": "dir"
		},
		{
			"path": "pingy.json",
